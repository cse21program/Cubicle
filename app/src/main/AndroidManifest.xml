<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cubicle">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Cubicle"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SignUpActivity" />
        <activity android:name=".SignInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MessageActivity" />
        <activity android:name=".PostAndChatActivity" />
        <activity android:name=".ImageViewerActivity" />
        <activity android:name=".LikeAndCommentActivity" />
        <activity android:name=".EventRegistrationActivity" />
        <activity android:name=".ClubProfileActivity" />
        <activity
            android:name=".DevelopersActivity"
            android:parentActivityName=".ProfileActivity" />
        <activity
            android:name=".SettingActivity"
            android:parentActivityName=".ProfileActivity" />
        <activity
            android:name=".FeedbackActivity"
            android:parentActivityName=".ProfileActivity" />
        <activity android:name=".ProfileActivity" />
    </application>

</manifest>